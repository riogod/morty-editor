{
  "name": "morty-editor",
  "version": "0.0.1",
  "description": "React canvas editor for text and drawing",
  "engines": {
    "node": ">=8.x"
  },
  "keywords": [
    "react",
    "react-component",
    "react-text-editor",
    "canvas-editor",
    "draw-editor",
    "canvas",
    "text",
    "editor",
    "morty-editor"
  ],
  "homepage": "https://github.com/riogod/morty-editor",
  "repository": {
    "type": "git",
    "url": "git@github.com:riogod/morty-editor.git"
  },
  "bugs": {
    "url": "https://github.com/riogod/morty-editor/issues"
  },
  "files": [
    "assets/*.css",
    "lib",
    "es",
    "dist"
  ],
  "license": "MIT",
  "main": "./lib/index",
  "module": "./es/index",
  "style": "./assets/index.css",
  "scripts": {
    "start": "cross-env NODE_ENV=development father doc dev --storybook",
    "build": "father doc build --storybook",
    "compile": "father build && lessc assets/index.less assets/index.css",
    "gh-pages": "npm run build && father doc deploy",
    "prepublishOnly": "npm run compile && np --yolo --no-publish",
    "postpublish": "npm run gh-pages",
    "lint": "eslint src/ --ext .ts,.tsx,.jsx,.js,.md",
    "prettier": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "test": "father test",
    "coverage": "father test --coverage"
  },
  "peerDependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "dependencies": {
    "@babel/runtime": "^7.10.2",
    "classnames": "^2.2.6"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.10",
    "@types/jest": "^25.2.3",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "@umijs/fabric": "^2.0.9",
    "cross-env": "^7.0.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.5.0",
    "eslint": "^7.1.0",
    "father": "^2.29.3",
    "less": "^3.11.2",
    "np": "^6.2.3",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "typescript": "^3.6.0"
  }
}
